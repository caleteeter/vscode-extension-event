{
    "name": "vscode-event-emitter",
    "displayName": "Event Producer",
    "description": "Event Producer extension",
    "version": "0.1.0",
    "publisher": "ctmsft",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Extension Packs",
        "Other"
    ],
    "activationEvents": [
        "onCommand:sample.eventProducerCommand"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "sample.eventProducerCommand",
                "title": "Sample Event Producer"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile && npm run webpack",
        "webpack": "webpack --profile --json --mode development > webpack-stats.json",
        "webpack-prod": "webpack --profile --json --mode production > webpack-stats-prod.json",
        "package": "npx vsce package",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^12.12.31",
        "ts-loader": "^6.2.2",
        "tslint": "^6.1.0",
        "typescript": "^3.8.3",
        "vscode": "^1.1.36",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "vscode-azureextensionui": "^0.29.12"
    }
}
